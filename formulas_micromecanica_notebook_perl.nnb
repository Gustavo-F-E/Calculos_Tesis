                "const Table2 = require('cli-table');\r\n\r\nconst propiedades2 = [\r\n    { propiedad: 'Módulo elástico de la matriz', valor: E_m, unidades: 'GPa' },\r\n    { propiedad: 'Módulo de Poisson de la matriz', valor: NU_m, unidades: '--' },\r\n    { propiedad: 'Módulo de corte de la matriz', valor: G_m, unidades: 'GPa' },\r\n    { propiedad: 'Fracción en Volumen de la matriz', valor: V_m, unidades: '%' }\r\n    ];\r\n\r\n// Crear una tabla con bordes\r\nconst tabla_propiedades2 = new Table2({\r\n    head: ['Propiedad', 'Valor', 'Unidades'],\r\n    colWidths: [60, 20, 20] // ajusta el ancho de las columnas según sea necesario\r\n});\r\n\r\n// Agregar datos a la tabla\r\npropiedades2.forEach(prop => {\r\n    tabla_propiedades2.push([prop.propiedad, prop.valor, prop.unidades]);\r\n})\r\n\r\n// Imprimir la tabla\r\nconsole.log(tabla_propiedades2.toString());"
